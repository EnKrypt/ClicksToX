{
  "name": "clicks-to-x-extension",
  "version": "1.0.0",
  "description": "A browser game where your lobby races to get from one Wikipedia page to another in the fewest clicks",
  "author": "Arvind Kumar <arvind@enkrypt.in> (https://arvind.io)",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/ dist/",
    "build": "sh build-all.sh",
    "dev": "nodemon --config nodemon.json src/main.tsx src/background.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-d3-tree": "^3.6.2",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.267",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
